{
  "Cors": {
    "AllowedMethods": [ "GET" ],
    "AllowedOrigins": [ "http://localhost:8080", "http://127.0.0.1:8080" ]
  },
  "EnableMigrations": true,
  "EnableOpenApi": true,
  "GraphQL": {
    "EnableMetrics": true,
    "ExposeExceptionDetails": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "POSTGRESQLCONNSTR_Wishes": "",
  "SQLCONNSTR_Wishes": "",
  "UseGraphQLAltair": true,
  "UseGraphQLGraphiQL": true,
  "UseGraphQLPlayground": true,
  "UseGraphQLVoyager": true
}
